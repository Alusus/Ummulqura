import "hijri.alusus";

اشمل "مـتم/متم"؛
استخدم مـتم

عرف امـ_القرى: لقب Umalqurra؛
@دمج وحدة امـ_القرى {
    عرف تـاريخ: لقب Date
    @دمج صنف تـاريخ {
        عرف السنة: لقب year؛
        عرف الشهر: لقب month ؛
        عرف اليوم: لقب day ؛
        عرف طول_الشهر: لقب monthLen ؛
        
    
    }  
    عرف ميلادي_الى_الهجري: لقب gegorean_to_hijri؛
  
    عرف هجري_الى_ميلادي: لقب  hijri_to_gregorian؛
  
    عرف جوليان_الى_ميلادي: لقب julian_to_gregorian؛
    
 
}

